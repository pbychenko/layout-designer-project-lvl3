mixin contact(userData)
  li.contact.list-group-item.py-0.shadow-sm
    a(href="#").text-decoration-none.link-dark
      .d-flex.align-items-center
        img(src=userData.avatar).user-image.rounded-circle
        .contact-data.p-4
          p.user-name.mb-0
            strong= userData.name
          p.user-status.small.mb-0= userData.status